	},
	"criteria": {
		"galleys": {
			"label": {"like": "%Ã  l'achat%"},
			"remote_url": {"like": "http%://%/${reference}/${chapter}"}
		},
		"settings": {
        	"locale": {"in": ["fr_FR","en_US"]},
        	"setting_name": {"in": ["abstract","title","subtitle"]}
		},
